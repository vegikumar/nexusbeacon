{
  "buildCommand": "npm run build",
  "runtime": "php:8.2",
  "env": [
    "APP_KEY",
    "APP_DEBUG",
    "APP_ENV",
    "APP_URL"
  ],
  "functions": {
    "api/index.php": {
      "runtime": "php:8.2"
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}
